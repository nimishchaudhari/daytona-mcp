  },
  "dependencies": {
    "@daytonaio/sdk": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.6.1",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.6",
    "@types/node": "^18.18.6",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
